{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }],
    "@babel/react"
  ],
  "plugins": [
    ["@babel/plugin-transform-typescript", { "isTSX": true }],
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-proposal-export-namespace-from",
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "test": {
      "presets": ["@babel/env"]
    }
  }
}
